<div class="container">
  <div class="row">
    <div class="col-xs-5 col-sm-Offset-4">
      <div class="title">
        <h1>Selection de photographe</h1>
      </div>
        <% @photographes.each do |photographe| %>
          <div class="card-photographe">
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcSM5EMrcA9A-beZXQnX8iQ3b5VcI1ncgriOdT74WVHBhUmGaV2j" />
            <div class="card-photographe-infos">
              <h2><%= photographe.nom %></h2>
              <h3><%= photographe.specialite %></h3>
              <p><%= photographe.annonce %></p>
            </div>
          </div>
        <% end %>
    </div>
  </div>
</div>
